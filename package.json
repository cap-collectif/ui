{
  "version": "1.8.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsdx watch",
    "build": "tsdx build",
    "ts:check": "tsc --noEmit",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src",
    "prepare": "run-s husky:install build",
    "size-limit:build": "yarn install --ignore-scripts && yarn run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "husky:install": "husky install",
    "chromatic": "npx chromatic --skip '@(renovate/**|dependabot/**|refs/tags/**)' --project-token CHROMATIC_PROJECT_TOKEN"
  },
  "peerDependencies": {
    "@styled-system/css": "^5.0.0",
    "framer-motion": ">=4.0.0",
    "moment": "^2.29.1",
    "react": ">=16",
    "styled-components": "^5.0.0"
  },
  "name": "@cap-collectif/ui",
  "author": "Cap Collectif <tech@cap-collectif.com>",
  "module": "dist/ui.esm.js",
  "size-limit": [
    {
      "path": "dist/ui.cjs.production.min.js",
      "limit": "350 KB"
    },
    {
      "path": "dist/ui.esm.js",
      "limit": "350 KB"
    }
  ],
  "devDependencies": {
