    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "run-s": "^0.0.0",
    "size-limit": "^4.12.0",
